{
    "name": "ws/file-info",
    "description": "A library to get file information.",
    "license": "MIT",
    "authors": [
        {
            "name": "Benjamin Georgeault",
            "email": "github@wedgesama.fr"
        }
    ],
    "keywords": [
        "file info",
        "mimetype",
        "mime type"
    ],
    "require": {
        "php": ">=5.3.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.6"
    },
    "autoload": {
        "psr-4": {
            "WS\\Libraries\\FileUtil\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WS\\Libraries\\FileUtil\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
